{
  "name": "depmender",
  "version": "1.3.0",
  "description": "A CLI tool that scans JavaScript/TypeScript projects for dependency issues and fixes them",
  "main": "dist/index.js",
  "bin": {
    "depmender": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:prod": "tsc --declaration false --declarationMap false --sourceMap false",
    "dev": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepare": "npm run build:prod"
  },
  "keywords": [
    "dependency",
    "npm",
    "yarn",
    "pnpm",
    "security",
    "cli",
    "package-manager",
    "vulnerability",
    "outdated",
    "audit"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@eslint-community/eslint-utils": "^4.9.1",
